generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id    Int     @id @default(autoincrement())
  username  String? @db.VarChar(100) @unique
  email String? @db.VarChar(100) @unique
  password String 
  createdAt DateTime @default(now())


}
